    "# if, else, and elif\n",
    "\n",
    "x = int(input(\"Pick a number: \"))\n",
    "if x > 10:\n",
    "    print(\"That's a big number!\")\n",
    "elif x == 10:\n",
    "    print(\"Nice! You picked the same number.\")\n",
    "else:\n",
    "    print(\"That number is pretty small.\")"
   ]
